{
  "name": "@voltagent/task-manager",
  "version": "0.1.0",
  "description": "Task management system for VoltAgent",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "voltagent-task": "./dist/bin/task-manager.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "biome check src",
    "lint:fix": "biome check --apply src",
    "test": "jest"
  },
  "dependencies": {
    "@voltagent/core": "workspace:*",
    "@voltagent/mcp-server": "workspace:*",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "inquirer": "^12.5.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.8",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
