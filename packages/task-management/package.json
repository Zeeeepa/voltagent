{
  "name": "@voltagent/task-management",
  "version": "0.1.0",
  "description": "Task management system for AI-driven development",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "biome check .",
    "lint:fix": "biome check . --apply",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "attw": "attw --pack .",
    "publint": "publint"
  },
  "dependencies": {
    "@voltagent/core": "workspace:*",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@types/node": "^20.11.30",
    "publint": "^0.3.8",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.2",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": ">=18"
  }
}

