{
  "name": "@voltagent/static-analysis",
  "version": "0.1.0",
  "description": "Static analysis tools for code quality and unused function detection",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check . --apply",
    "publint": "publint",
    "attw": "attw --pack ."
  },
  "dependencies": {
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.6",
    "@babel/types": "^7.23.6",
    "@typescript-eslint/parser": "^6.15.0",
    "@typescript-eslint/typescript-estree": "^6.15.0",
    "acorn": "^8.11.2",
    "acorn-walk": "^8.3.1",
    "esprima": "^4.0.1",
    "typescript": "^5.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.4",
    "@types/node": "^20.10.5",
    "tsup": "^8.0.1",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "@voltagent/core": "workspace:*"
  },
  "keywords": [
    "static-analysis",
    "ast",
    "unused-functions",
    "code-quality",
    "typescript",
    "javascript",
    "python",
    "go"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zeeeepa/voltagent.git",
    "directory": "packages/static-analysis"
  }
}

