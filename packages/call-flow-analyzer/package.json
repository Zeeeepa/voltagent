    "@typescript-eslint/typescript-estree": "^6.0.0",
    "acorn": "^8.11.0",
    "acorn-walk": "^8.3.0",
    "typescript": "^5.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
