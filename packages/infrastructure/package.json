{
  "name": "@voltagent/infrastructure",
  "version": "1.0.0",
  "description": "SQL Database & Workflow Orchestration Module for VoltAgent's PR analysis system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "voltagent",
    "infrastructure",
    "database",
    "workflow",
    "orchestration",
    "postgresql",
    "redis",
    "task-queue",
    "codegen"
  ],
  "author": "VoltAgent Team",
  "license": "MIT",
  "dependencies": {
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.6",
    "@types/node-cron": "^3.0.11",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Zeeeepa/voltagent.git",
    "directory": "packages/infrastructure"
  },
  "bugs": {
    "url": "https://github.com/Zeeeepa/voltagent/issues"
  },
  "homepage": "https://github.com/Zeeeepa/voltagent#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src"],
    "testMatch": ["**/__tests__/**/*.test.ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/__tests__/**"
    ]
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "warn"
    }
  }
}

