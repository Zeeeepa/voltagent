{
  "name": "@voltagent/comprehensive-cicd",
  "version": "1.0.0",
  "description": "Comprehensive CI/CD System with Codegen Integration for intelligent prompt generation and seamless PR creation",
  "main": "index.js",
  "bin": {
    "comprehensive-cicd": "./index.js"
  },
  "scripts": {
    "start": "node index.js start",
    "dev": "NODE_ENV=development nodemon index.js start",
    "health": "node index.js health",
    "metrics": "node index.js metrics",
    "init-db": "node index.js init-db",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "docker:build": "docker-compose -f ../../docker-compose.comprehensive-cicd.yml build",
    "docker:up": "docker-compose -f ../../docker-compose.comprehensive-cicd.yml up -d",
    "docker:down": "docker-compose -f ../../docker-compose.comprehensive-cicd.yml down",
    "docker:logs": "docker-compose -f ../../docker-compose.comprehensive-cicd.yml logs -f"
  },
  "keywords": [
    "cicd",
    "codegen",
    "automation",
    "ai",
    "prompt-generation",
    "pr-creation",
    "workflow-orchestration",
    "voltagent"
  ],
  "author": "Voltagent Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zeeeepa/voltagent.git",
    "directory": "src/comprehensive_cicd"
  },
  "bugs": {
    "url": "https://github.com/Zeeeepa/voltagent/issues"
  },
  "homepage": "https://github.com/Zeeeepa/voltagent#readme",
  "files": [
    "api/",
    "services/",
    "database/",
    "workflows/",
    "monitoring/",
    "index.js",
    "README.md"
  ],
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!jest.config.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "node/no-unpublished-require": "off"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "none",
    "printWidth": 100
  }
}

