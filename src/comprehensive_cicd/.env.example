# Comprehensive CI/CD System Configuration

# Server Configuration
PORT=3000
NODE_ENV=development
API_VERSION=v1

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/comprehensive_cicd
DB_HOST=localhost
DB_PORT=5432
DB_NAME=comprehensive_cicd
DB_USER=username
DB_PASSWORD=password
DB_SSL=false
DB_POOL_MIN=2
DB_POOL_MAX=10

# Claude Code Validation Configuration
AGENTAPI_URL=http://localhost:8000
CLAUDE_CODE_API_KEY=your-claude-code-api-key
CLAUDE_CODE_PATH=/usr/local/bin/claude

# WSL2 Configuration
WSL2_DISTRO=Ubuntu-22.04
WSL2_BASE_PATH=/mnt/c/projects
MAX_WSL2_INSTANCES=5
WSL2_MEMORY=8GB
WSL2_PROCESSORS=4
WSL2_SWAP=2GB
WSL2_LOCALHOST_FORWARDING=true

# Validation Configuration
VALIDATION_TIMEOUT=300000
ENABLE_SECURITY_ANALYSIS=true
ENABLE_PERFORMANCE_ANALYSIS=true
CODE_QUALITY_WEIGHT=0.3
FUNCTIONALITY_WEIGHT=0.4
TESTING_WEIGHT=0.2
DOCUMENTATION_WEIGHT=0.1

# Deployment Configuration
DEPLOYMENT_STRATEGY=wsl2
ENABLE_CONTAINER_DEPLOYMENT=false
CONTAINER_REGISTRY=docker.io
CONTAINER_NAMESPACE=codegen-cicd

# Monitoring Configuration
ENABLE_METRICS=true
METRICS_PORT=9090
LOG_LEVEL=info
ENABLE_HEALTH_CHECKS=true

# Security Configuration
JWT_SECRET=your-jwt-secret-key
API_RATE_LIMIT=100
CORS_ORIGIN=*
ENABLE_HELMET=true

# GitHub Integration
GITHUB_TOKEN=your-github-token
GITHUB_WEBHOOK_SECRET=your-webhook-secret

# Notification Configuration
SLACK_WEBHOOK_URL=your-slack-webhook-url
EMAIL_SMTP_HOST=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_USER=your-email@example.com
EMAIL_PASSWORD=your-email-password

